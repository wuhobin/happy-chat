<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wuhobin.mapper.UserInfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.wuhobin.dataobject.UserInfoDO">
        <id column="id" property="id" />
        <result column="nick_name" property="nickName" />
        <result column="phone_num" property="phoneNum" />
        <result column="sex" property="sex" />
        <result column="avatar" property="avatar" />
        <result column="status" property="status" />
        <result column="login_ip" property="loginIp" />
        <result column="city" property="city" />
        <result column="login_date" property="loginDate" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, nick_name, phone_num, sex, avatar, status, login_ip, city, login_date, create_time, update_time
    </sql>

</mapper>
